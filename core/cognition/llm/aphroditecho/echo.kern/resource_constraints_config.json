{
  "description": "Resource constraint configuration for Deep Tree Echo State Networks (DTESN)",
  "version": "2.2.2",
  "target_environment": "neuromorphic_embedded",
  
  "constraints": {
    "cpu_primary": {
      "resource_type": "cpu_cycles",
      "max_allocation": 1000000000,
      "min_reserved": 100000000,
      "hard_limit": true,
      "description": "Primary CPU cycles constraint for embedded neuromorphic system"
    },
    "memory_main": {
      "resource_type": "memory_bytes", 
      "max_allocation": 536870912,
      "min_reserved": 67108864,
      "hard_limit": true,
      "description": "Main memory constraint (512MB total, 64MB reserved)"
    },
    "energy_budget": {
      "resource_type": "energy_joules",
      "max_allocation": 1.0,
      "min_reserved": 0.1,
      "hard_limit": false,
      "description": "Energy budget per allocation period (1J with 100mJ reserved)"
    },
    "neuromorphic_units": {
      "resource_type": "neuromorphic_units",
      "max_allocation": 64,
      "min_reserved": 8,
      "hard_limit": true,
      "description": "Available neuromorphic processing units"
    },
    "gpu_memory": {
      "resource_type": "gpu_memory",
      "max_allocation": 4294967296,
      "min_reserved": 1073741824,
      "hard_limit": true,
      "description": "GPU memory constraint (4GB total, 1GB reserved)"
    },
    "bandwidth_network": {
      "resource_type": "bandwidth_bps",
      "max_allocation": 100000000,
      "min_reserved": 10000000,
      "hard_limit": false,
      "description": "Network bandwidth constraint (100Mbps total)"
    }
  },
  
  "energy_model": {
    "base_power_watts": 0.005,
    "description": "5mW base power consumption for neuromorphic hardware",
    "operation_costs": {
      "membrane_evolution": 0.05,
      "bseries_computation": 0.5,
      "esn_state_update": 5.0,
      "context_switch": 0.025,
      "attention_allocation": 0.1,
      "tree_classification": 0.01,
      "oeis_validation": 0.02
    },
    "notes": "Energy costs in microjoules (Î¼J) for each operation type"
  },
  
  "realtime_constraints": {
    "membrane_evolution": {
      "max_duration_us": 10,
      "deadline_us": 15,
      "priority": 10,
      "description": "Membrane computing evolution - highest priority"
    },
    "bseries_computation": {
      "max_duration_us": 100,
      "deadline_us": 150,
      "priority": 8,
      "description": "B-Series tree classification and differential computation"
    },
    "esn_state_update": {
      "max_duration_us": 1000,
      "deadline_us": 1500,
      "priority": 6,
      "description": "Echo State Network reservoir state update"
    },
    "context_switch": {
      "max_duration_us": 5,
      "deadline_us": 10,
      "priority": 12,
      "description": "Kernel context switch - critical for real-time performance"
    },
    "attention_allocation": {
      "max_duration_us": 50,
      "deadline_us": 100,
      "priority": 7,
      "description": "ECAN attention resource allocation"
    }
  },
  
  "system_limits": {
    "max_concurrent_agents": 100,
    "max_operations_per_second_global": 10000,
    "memory_fragmentation_threshold": 0.25,
    "energy_warning_threshold": 0.8,
    "constraint_violation_alert_threshold": 0.1
  },
  
  "agent_classes": {
    "critical": {
      "priority_level": 10,
      "energy_budget_joules": 0.1,
      "max_operations_per_second": 1000,
      "description": "Critical system agents (safety, real-time control)"
    },
    "normal": {
      "priority_level": 5,
      "energy_budget_joules": 0.05,
      "max_operations_per_second": 500,
      "description": "Normal operational agents (learning, inference)"
    },
    "background": {
      "priority_level": 1,
      "energy_budget_joules": 0.01,
      "max_operations_per_second": 100,
      "description": "Background processing agents (optimization, maintenance)"
    }
  },
  
  "integration_settings": {
    "ecan_integration": {
      "enabled": true,
      "attention_bridge_path": "../echo.files/ECAN_RESOURCE_ALLOCATORS.md",
      "economic_model": "rent_collection",
      "description": "Integration with existing ECAN resource allocators"
    },
    "dtesn_integration": {
      "enabled": true,
      "oeis_validation": true,
      "membrane_computing": true,
      "bseries_computation": true,
      "esn_reservoir": true,
      "description": "Integration with DTESN kernel components"
    },
    "aphrodite_integration": {
      "enabled": true,
      "scheduler_integration": true,
      "memory_management": true,
      "description": "Integration with Aphrodite Engine processing scheduler"
    }
  },
  
  "monitoring": {
    "performance_tracking": true,
    "constraint_violation_logging": true,
    "energy_consumption_tracking": true,
    "realtime_metrics": true,
    "resource_utilization_history": true,
    "alert_thresholds": {
      "cpu_utilization": 0.9,
      "memory_utilization": 0.85,
      "energy_consumption_rate": 0.8,
      "constraint_violation_rate": 0.05
    }
  },
  
  "validation": {
    "oeis_a000081_compliance": true,
    "dtesn_timing_requirements": true,
    "neuromorphic_hardware_limits": true,
    "mathematical_correctness": true,
    "description": "Validation requirements for resource constraint enforcement"
  },
  
  "development": {
    "mock_components": true,
    "test_mode_enabled": false,
    "debug_logging": true,
    "constraint_bypass_for_testing": false,
    "description": "Development and testing configuration options"
  }
}