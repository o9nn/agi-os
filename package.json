{
  "name": "deep-tree-echo-agi-avatar",
  "version": "1.0.0",
  "description": "Advanced AGI Agent Avatar System with Deep Tree Echo cognitive architecture, Live2D Cubism SDK and Three.js 3D rendering",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./dist/styles/output.css",
    "dev": "ts-node-dev --respawn src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "node dist/index.js",
    "docker:build": "docker build -t deep-tree-echo-avatar .",
    "docker:run": "docker run -p 3000:3000 deep-tree-echo-avatar",
    "clean": "rm -rf dist node_modules",
    "prepare": "husky install"
  },
  "keywords": [
    "agi",
    "avatar",
    "live2d",
    "cubism",
    "threejs",
    "deep-tree-echo",
    "cognitive-architecture",
    "ai",
    "llm"
  ],
  "author": "9cog",
  "license": "AGPL-3.0",
  "dependencies": {
    "@cubism-sdk/core": "^5.0.0",
    "@cubism-sdk/framework": "^5.0.0",
    "@types/three": "^0.160.0",
    "three": "^0.160.0",
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "ws": "^8.16.0",
    "uuid": "^9.0.1",
    "rxjs": "^7.8.1",
    "lodash": "^4.17.21",
    "eventemitter3": "^5.0.1",
    "pixi.js": "^7.3.3",
    "pixi-live2d-display": "^0.4.0",
    "lru-cache": "^10.2.0",
    "pino": "^8.18.0",
    "dotenv": "^16.4.1",
    "zod": "^3.22.4",
    "hnswlib-node": "^3.0.0",
    "tiktoken": "^1.0.12"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
