      },
      "./create": {
        "types": "./dist/create.d.ts",
        "default": "./dist/create.js"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "pkgroll",
    "sync": "tsx scripts/sync.ts",
    "test": "vitest run"
  },
  "dependencies": {
    "@xsai-ext/shared-providers": "workspace:~",
    "@xsai/shared": "workspace:~"
  },
  "devDependencies": {
    "scule": "^1.3.0"
  }
}
