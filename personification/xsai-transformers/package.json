{
  "name": "@xsai-transformers/root",
  "type": "module",
  "version": "0.0.10",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "repository": "github:moeru-ai/xsai-transformers",
  "scripts": {
    "build": "pnpm run packages:build && pnpm -r -F=\"./playground\" run build",
    "dev": "pnpm run packages:build && pnpm -r -F=\"./playground\" run dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "nolyfill": "pnpm dlx nolyfill",
    "packages:build": "pnpm -r -F=\"./packages/*\" -F=\"./packages-top/*\" run build",
    "postinstall": "npx simple-git-hooks",
    "sizecheck": "npx vite-bundle-visualizer",
    "test": "vitest --coverage",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "up": "taze -w -r -I -f && pnpm prune && pnpm dedupe"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@antfu/ni": "^26.2.0",
    "@antfu/nip": "^0.1.0",
    "@importantimport/eslint-config": "^1.0.0-beta.2",
    "@importantimport/tsconfig": "^1.0.0-beta.2",
    "@pnpm/find-workspace-dir": "^1000.1.3",
    "@types/audioworklet": "^0.0.87",
    "@types/node": "^24.8.1",
    "@unocss/eslint-config": "^66.5.4",
    "@unocss/eslint-plugin": "^66.5.4",
    "@vitest/coverage-v8": "3.0.5",
    "@webgpu/types": "^0.1.66",
    "bumpp": "^10.3.1",
    "changelogithub": "^13.16.0",
    "eslint": "^9.38.0",
    "lint-staged": "^16.2.4",
    "pkgroll": "^2.20.0",
    "rollup": "^4.52.5",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.8.0",
    "typescript": "~5.9.3",
    "unocss": "^66.5.4",
    "vite": "^7.1.10",
    "vite-plugin-inspect": "^11.3.3",
    "vitest": "^3.2.4"
  },
  "workspaces": [
    "packages/**",
    "playground"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "onnxruntime-node",
      "sharp",
      "simple-git-hooks",
      "unrs-resolver"
    ],
    "overrides": {
      "array-flatten": "npm:@nolyfill/array-flatten@^1.0.44",
      "axios": "npm:feaxios@^0.0.23",
      "is-core-module": "npm:@nolyfill/is-core-module@^1.0.39",
      "isarray": "npm:@nolyfill/isarray@^1.0.44",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1.0.44",
      "safer-buffer": "npm:@nolyfill/safer-buffer@^1.0.44",
      "side-channel": "npm:@nolyfill/side-channel@^1.0.44",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1.0.44"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
