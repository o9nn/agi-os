{
  "name": "cogutil",
  "version": "2.0.3",
  "description": "OpenCog utility library - Low-level C++ utilities for the OpenCog framework",
  "homepage": "https://github.com/opencog/cogutil",
  "license": "AGPL-3.0",
  "supports": "windows & linux & osx",
  "dependencies": [
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "boost",
      "features": ["system", "filesystem", "program-options", "thread"]
    }
  ]
}
